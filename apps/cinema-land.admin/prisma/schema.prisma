generator client {
  provider = "prisma-client-js"
  previewFeatures = ["postgresqlExtensions"]
}
 
datasource db {
  provider = "postgresql"
  url = env("POSTGRES_PRISMA_URL")
  extensions = [uuidOssp(map: "uuid-ossp")]
}

model Movie {
  id                String   @id @default(dbgenerated("public.uuid_generate_v4()")) @db.Uuid
  title             String
  genre             String
  yearOfIssue       Int
  @@map("movies")
}
